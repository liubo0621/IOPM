.checkMask.hotSpot .newsContent .Hotspot{
	margin: 20px 5px;
}
.checkMask.hotSpot .newsContent .image{
	width: 100%; height: 300px;
}
.checkMask.hotSpot .newsContent p{
	text-align: left;
	text-indent: 2em;
	margin-bottom: 5px;
}
.sets{
	text-align: center;
	position: relative;
	height: 20px;
}
.sets .views label,.sets .date span.todayHot{
	margin-right: 10px;
}
.sets .views label,.sets .views input{
	display: inline-block;
	color: white;
	font-size: 16px;
}
.sets .date{
	position: absolute;
	right: 0; bottom: 0;
}
.sets .date span{
	color: #FFC107;
	font-size: 16px;
	vertical-align: middle;
}
.sets .date span.todayHot{
	cursor: pointer;
	display: none;
}
.sets .date span.todayHot:hover{
	color: red;
}

.checkMask form p {
	padding:0 5px;
}

/*弹出窗*/
.comment {
	width: 90%;
	margin: 0 5%;
}
.comment-Top ul{
	width: 100%;
	height: 30px;
	line-height: 30px;
	font-size:14px;
}

.comment-Top ul li {
	float: left;
}
.comment-Top img {
	width: 10px;
	height: 5px;
	margin: 0 5px;
	cursor: pointer;
}


.comment-Top ul li:nth-child(2){
	margin-left:25%;
}
.comment-Top ul li:nth-child(3){
	margin-left:25%;
}

.comment-content {
	width: 90%;
	margin: 5px 5%;
	border:1px solid #ccc;
}
.comment-C {
	width: 100%;
	text-align: left;
	padding: 0 5px;
	border-bottom:1px dashed #ccc;
}
.comment-C div,ul {
	width: 130%;
}
.comment-C ul li {
	float: left;
	margin-right:10px;
}

.Hot {
	width:90%;
	height:auto;
	margin-left:5%;
}

.svg {
	position: relative;
	width: 90%;
	height: 20px;
	line-height: 20px;
	margin:0 5% 15px;
}
.svg .svg-wen {
	height: 20px;
	line-height: 20px;
	float: left;
}
.svg .svg-img {
	position: absolute;
	height: 20px;
	cursor: pointer;
}
.svg div:nth-child(1){
	right: 180px;
}
.svg div:nth-child(2){
	right: 80px;
}
.svg img {
	width: 20px;
	height: 20px;
	margin-right:2px;
}

.clear:after {
	content:'';
	height: 0;
	line-height: 0;
	display: block;
	clear:both;
	visibility: hidden;
}
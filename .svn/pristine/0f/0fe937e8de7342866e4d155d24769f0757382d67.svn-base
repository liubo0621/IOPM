}


.srhList{
	width: 168px;
    height: 130px;
    position: absolute;
    left: 394px;
    top: 260px;
    z-index: 99;
    background: rgb(255, 255, 255);
    padding: 5px;
    border: 1px solid rgb(201, 197, 198);
    border-radius: 3px;
    display: none;
    overflow: hidden;
}
.srhList li{
	white-space: normal;
    overflow: hidden;
	margin-bottom: 2px;

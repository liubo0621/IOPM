#data-table thead th,#data-table tbody tr{
	cursor: pointer;
	text-align: center;
}
#data-table tbody tr:hover{
	background: #e2e7eb;
}

#data-table tbody tr td.move{
	background: #717070;
	color: #fff;
}

.checkMask table td{
	padding: 5px;
}

#data-table,#userFormId table{
	font-size:13px!important;
}
@media only screen and (max-width: 1450px) {
	.top .search input[type="text"],.top .search i,.top .search i em,.top .search select{
		width: 120px
	}
	.top .search i em{
		background: url(../imgs/selectArrowdown.png) 90px center no-repeat;
	}
	.top .btns input{
		width: 70px;
	}
}

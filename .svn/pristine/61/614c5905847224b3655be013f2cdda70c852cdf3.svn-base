package cn.com.pattek.user.entity;

import java.util.Date;

public class Notice {
	private Long id;
	private String url;
	private String title;
	private String content;
	private Date release_time;
	private Date record_time;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Date getRelease_time() {
		return release_time;
	}

	public void setRelease_time(Date releaseTime) {
		release_time = releaseTime;
	}

	public Date getRecord_time() {
		return record_time;
	}

	public void setRecord_time(Date recordTime) {
		record_time = recordTime;
	}

	@Override
	public String toString() {
		return "Notice [content=" + content + ", id=" + id + ", record_time="
				+ record_time + ", release_time=" + release_time + ", title="
				+ title + ", url=" + url + "]";
	}

}

/*类别*/
.category {
    width: 100%;
    /*height: 375px;*/
    margin: 10px 0;
}
/*一级标题*/
.menu {
    float: left;
    width: 200px;
    height: 200px;
    margin: 0 23px 10px 23px;
}

.menu-theme {
    position: relative;
    width: 200px;
    height: 200px;
    border-radius:10px;
    box-shadow: 3px 5px 10px #888888;
}

.menu-theme .photo {
    width: 140px;
    height: 140px;
    margin: 8px 28px;
    /*border-radius: 50%;*/
}

.menu-theme .nore {
    position: absolute;
    bottom: 0;
    right: 0;
}

.menu-theme .close {
    position: absolute;
    bottom: 0;
    right: 0;
}

.menu-theme span {
    display: block;
    margin-top:10px;
    font-size: 20px;
    text-align: center;
}

.menu-theme i {
    font-size:20px;
}
/*二级类别*/
.submenu {
    z-index: 1;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 210px;
    background-color: #fff;
    margin-bottom:10px;
    display: none;
    box-shadow: 3px 5px 10px #888888;
   overflow:hidden;
}
.submenu .scrollBox {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 5px;
    background-color: #ccc;
}

.submenu .bar {
    position: absolute;
    left: 0;
    height:4px;
    cursor: pointer;
    border-radius: 10px;
    background-color: #000;
}

.submenu .bar:hover {
    height: 8px;
}

package cn.com.pattek.RelatedNews.entity;

public class Article {
	private Long id;    //线索ID
    private String keywords; //线索
    private Long count;  //总量
    private String emotion;  //情感倾向
    private Long positive;  //正面
    private Long negative;  //负面
    private Long neutral;   //中立
    private Long rn;  //编号
    private String name;
    private String clues_ids;
    private Long may_invalid;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getKeywords() {
		return keywords;
	}
	public void setKeywords(String keywords) {
		this.keywords = keywords;
	}
	public Long getCount() {
		return count;
	}
	public void setCount(Long count) {
		this.count = count;
	}
	public String getEmotion() {
		return emotion;
	}
	public void setEmotion(String emotion) {
		this.emotion = emotion;
	}
	public Long getRn() {
		return rn;
	}
	public void setRn(Long rn) {
		this.rn = rn;
	}
	public Long getPositive() {
		return positive;
	}
	public void setPositive(Long positive) {
		this.positive = positive;
	}
	public Long getNegative() {
		return negative;
	}
	public void setNegative(Long negative) {
		this.negative = negative;
	}
	public Long getNeutral() {
		return neutral;
	}
	public void setNeutral(Long neutral) {
		this.neutral = neutral;
	}
	public String getClues_ids() {
		return clues_ids;
	}
	public void setClues_ids(String cluesIds) {
		clues_ids = cluesIds;
	}
	public Long getMay_invalid() {
		return may_invalid;
	}
	public void setMay_invalid(Long mayInvalid) {
		may_invalid = mayInvalid;
	}
	
}
